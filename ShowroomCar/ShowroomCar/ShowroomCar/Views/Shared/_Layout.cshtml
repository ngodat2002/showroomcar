
<!DOCTYPE HTML>
<html>
<head>
    <title>Showroom ô tô</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link href="~/App_Themes/client/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/App_Themes/client/css/form.css" rel="stylesheet" type="text/css" media="all" />
    <link href='https://fonts.googleapis.com/css?family=Exo+2' rel='stylesheet' type='text/css'>


    <script type="text/javascript" src="~/App_Themes/client/js/jquery1.min.js"></script>
    <!-- start menu -->
    <link href="~/App_Themes/client/css/megamenu.css" rel="stylesheet" type="text/css" media="all" />
    <script type="text/javascript" src="~/App_Themes/client/js/megamenu.js"></script>

    <!--start slider -->
    <link rel="stylesheet" href="~/App_Themes/client/css/fwslider.css" media="all">
    <style>
        .dropbtn {
            background-color: #4CAF50;
            color: white;
            padding: 16px;
            font-size: 16px;
            border: none;
            cursor: pointer;
        }

        .dropdown {
            position: relative;
            display: inline-block;
        }

        .dropdown-content {
            display: none;
            position: absolute;
            top: 30px;
            left: 122px;
            background-color: #f9f9f9;
            min-width: 160px;
            box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
            z-index: 1;
        }

        .dropdown-content1 {
            display: none;
            position: absolute;
            top: 30px;
            left: 214px;
            background-color: #f9f9f9;
            min-width: 160px;
            box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
            z-index: 1;
        }

        .dropdown-content a {
            color: black;
            padding: 12px 16px;
            text-decoration: none;
            display: block;
        }

        .dropdown-content1 a {
            color: black;
            padding: 12px 16px;
            text-decoration: none;
            display: block;
        }

        .dropdown:hover .dropbtn {
            background-color: #3e8e41;
        }

        a#hangXe:hover a {
            color: blue
        }

        a#hangXe:hover .dropdown-content {
            display: block;
        }

        a#soCho:hover .dropdown-content1 {
            display: block;
        }
    </style>
    <script src="~/App_Themes/client/js/jquery-ui.min.js"></script>
    <script src="~/App_Themes/client/js/css3-mediaqueries.js"></script>
    <script src="~/App_Themes/client/js/fwslider.js"></script>
    <!--end slider -->
    <script src="~/App_Themes/client/js/jquery.easydropdown.js"></script>
    @RenderSection("Scripts", false)
</head>
<body>
    <div class="header-top">
        <div class="wrap">
            <div class="cssmenu">
                <ul>
                </ul>
            </div>
            <div class="clear"></div>
        </div>
    </div>
    <div class="header-bottom">
        <div class="wrap">
            <div class="header-bottom-left">
                <div class="logo">
                    <a href="/"><img src="~/App_Themes/client/images/8.png" width="200px" alt="" /></a>
                </div>
                <div class="menu">
                    <ul class="megamenu skyblue">
                        <li class="active grid"><a href="/">Home</a></li>
                        <li>
                        </li>
                       
                    </ul>
                </div>
            </div>
            <div class="header-bottom-right">

                <div class="search">
                    @using (Html.BeginForm("Sreach", "Home", FormMethod.Get, new { @style = "position:relative" }))
                    {
                        <span>
                            @Html.TextBox("SearchString", ViewBag.CurrentFilter as string, null, new { @onfocus = "this.value = '';", @onblur = "if (this.value == '') { this.value = 'Tìm kiếm'; }", @class = "form-control", @placeholder = "Tìm kiếm ..." })
                            <input type="submit" value="Subscribe" id="submit" name="submit" style="position: absolute; top: 0;right: 0">
                            <div id="response"> </div>
                        </span>

                    }
                    @*<input type="text" name="s" class="textbox" value="Tìm kiếm" onfocus="this.value = '';" onblur="if (this.value == '') { this.value = 'Tìm kiếm'; }">
                        <input type="submit" value="Subscribe" id="submit" name="submit">
                        <div id="response"> </div>*@
                </div>            
            </div>
            <div class="clear"></div>
        </div>
    </div>

    <div class="main">


        @RenderBody()

    </div>
    <div class="footer">
        <div class="footer-top">
            <div class="wrap">
                <div class="section group example container">

                    <div class="row">
                        <div class="col-md-6">

                            <div class="col_1_of_2 span_1_of_2">
                                <ul class="f-list">
                                    <li><img src="~/App_Themes/client/images/8.png"><div class="clear"></div></li>
                                </ul>
                                <div id="fb-root"></div>
                                <script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v10.0" nonce="VbW55Qk2"></script>

                                <div class="col-lg-12 col-md-12 col-sm-12 p-3 border-top float-left">
                                    <div class="footer-widget mb-30 ml-75 text-center">
                                        <div class="footer-title">
                                            <h6 class="font-weight-bold text-white">THEO DÕI CHÚNG TÔI</h6>
                                        </div>
                                        <div class="footer-list">
                                            <div class="fb-page" data-href="https://www.facebook.com/105239151581073" data-tabs="timeline" data-width="" data-height="220" data-small-header="false" data-adapt-container-width="true" data-hide-cover="false" data-show-facepile="true"><blockquote cite="https://www.facebook.com/105239151581073" class="fb-xfbml-parse-ignore"><a href="https://www.facebook.com/105239151581073">Quanglinh Store</a></blockquote></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="col_1_of_2 span_1_of_2">

                                <div id="fb-root"></div>
                                <script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v10.0" nonce="VbW55Qk2"></script>

                                <div class="col-lg-12 col-md-12 col-sm-12 p-3 float-left">
                                    <div class="footer-widget mb-30 ml-75 text-center">
                                        <div class="footer-title">
                                            <h6 class="font-weight-bold text-white">VỊ TRÍ</h6>
                                        </div>
                                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d125400.29781708465!2d106.74873470251822!3d10.829722810725043!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3175275b3a69ce75%3A0xfc7063168fc12eaf!2zUXXhuq1uIDksIFRow6BuaCBwaOG7kSBI4buTIENow60gTWluaCwgVmnhu4d0IE5hbQ!5e0!3m2!1svi!2s!4v1619440161798!5m2!1svi!2s" width="600" height="320" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                    <div class="clear"></div>
                </div>
            </div>
        </div>

    </div>
</body>
</html>
